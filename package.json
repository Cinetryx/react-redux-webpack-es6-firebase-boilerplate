{
  "name": "React-Firebase-ES6-template",
  "version": "1.0.0",
  "description": "Boilerplate to build awesome React apps with Firebase authentication",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:prod": "webpack -p --config webpack.config.prod.js --progress --colors",
    "build": "npm run clean && npm run build:webpack",
    "dev": "SET NODE_ENV=development && webpack-dashboard -- node ./server/app.js",
    "debug": "node --inspect ./server/app.js",
    "start": "SET NODE_ENV=production&& node ./server/app.js",
    "postinstall": "webpack --config webpack.config.dev.js --progress --colors",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "set NODE_ENV=development && mocha --opts test/mocha.opts --bail",
    "test:watch": "npm test -- --watch --reporter list",
    "test:debug": "NODE_ENV=development && mocha --opts test/mocha.opts --debug-brk --inspect"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "firebase",
    "template",
    "boilerplate"
  ],
  "author": "Ranjith Nair <ranjith2112@gmail.com> (http://ranjithnair.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RanjithNair/react-redux-webpack-es6-firebase-boilerplate/issues"
  },
  "homepage": "https://github.com/RanjithNair/react-redux-webpack-es6-firebase-boilerplate",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.9.0",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.11",
    "expect": "^1.20.1",
    "file-loader": "^0.8.5",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "open": "0.0.5",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "reactotron-react-js": "^1.9.1",
    "reactotron-redux": "^1.9.1",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-mock-store": "^1.0.4",
    "sinon": "^2.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.5",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "firebase": "^3.0.3",
    "jquery": "^3.2.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
